{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "modules": false,
        "useBuiltIns": "entry",
      }
    ],
    "@babel/preset-react",
    ["@babel/preset-stage-0", { "loose": true, "decoratorsLegacy": true, "pipelineProposal": "minimal" }]
  ],
  "plugins": ["autobind-class-methods", "closure-elimination", "@knoopx/jsx-classnames"],
  "env": {
    "development": {
      "plugins": [ "react-hot-loader/babel"]
    },
  }
}
